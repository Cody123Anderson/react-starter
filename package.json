{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "React Starter Application",
  "main": "server.js",
  "scripts": {
    "dev": "export NODE_ENV=development && webpack --watch --progress",
    "lint": "eslint . --ext .js --fix && npm run lint:sass",
    "lint:sass": "sass-lint --config .sass-lint.yml --verbose --no-exit",
    "production": "NODE_ENV=production webpack --progress",
    "start": "export NODE_ENV=development && webpack-dev-server --port 3001 --progress",
    "test": "NODE_ENV=development mocha --compilers js:babel-core/register --require ignore-styles --require ./src/config/test-setup.js --recursive ./src/**/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cody123Anderson/react-starter.git"
  },
  "keywords": [
    "react"
  ],
  "author": "cody123anderson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Cody123Anderson/react-starter/issues"
  },
  "homepage": "https://github.com/Cody123Anderson/react-starter#readme",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-preset-env": "^1.3.2",
    "babel-preset-react": "^6.23.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "css-loader": "^0.28.0",
    "enzyme": "^2.8.0",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^6.10.3",
    "file-loader": "^0.11.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.12.0",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.2",
    "postcss-loader": "^1.3.3",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "sass-lint": "^1.10.2",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  }
}
